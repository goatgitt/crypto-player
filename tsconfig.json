{
  "compilerOptions": {
    "outDir": "./",
    "noImplicitAny": true,
    "sourceMap": true,
    "modules": ["es6", "esnext"],
    "target": "es5",
    "jsx": "react" && "preserve",
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["dom", "dom.iterable", "esnext"],
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "baseUrl": "."
  },
  "exclude": ["node_modules"],
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"]
}
